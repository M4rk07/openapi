{
    "name": "restz/openapi",
    "type": "library",
    "description": "OpenAPI V3 Reader and Writer.",
    "keywords": [
        "restz",
        "openapi"
    ],
    "homepage": "https://github.com/restz/openapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Ognjenovic",
            "email": "marko.ognjenovicgm@gmail.com"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Restz\\OpenAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Restz\\OpenAPI\\Tests": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
